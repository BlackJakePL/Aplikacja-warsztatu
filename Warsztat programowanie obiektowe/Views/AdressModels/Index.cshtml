@model IEnumerable<Warsztat_programowanie_obiektowe.Models.AdressModel>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model=> model.ID_adress)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.City)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ZipCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Street)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BuildingNo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FlatNo)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ID_adress)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.City)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ZipCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Street)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BuildingNo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FlatNo)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID_adress }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID_adress }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID_adress })
        </td>
    </tr>
}

</table>
